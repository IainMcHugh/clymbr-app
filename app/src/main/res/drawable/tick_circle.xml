<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/tick_circle">
        <shape android:shape="oval">
            <stroke android:color="#000000" android:width="5dp" />
            <solid android:color="#414141"/>
            <size android:width="150dp" android:height="150dp"/>
        </shape>
    </item>

    <item
        android:id="@+id/tick1"
        android:gravity="center">
        <shape android:shape="oval">
            <size android:width="7dp" android:height="7dp" />
            <stroke android:color="#1A000000" android:width="1dp" />
            <solid android:color="#800D6100"/>
        </shape>

    </item>

</layer-list>